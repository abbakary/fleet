  "step_pre_trip_documentation_title": "Nyaraka za Kabla ya Safari",
  "step_pre_trip_documentation_summary": "Thibitisha utambulisho wa gari, pata kusoma za msingi, na andika hali yoyote iliyopo kabla ya kuanza ukaguzi wa kimwili.",
  "step_pre_trip_documentation_instruction_1": "Skana VIN au nambari ya usajili kutumia overlay ya skana.",
  "step_pre_trip_documentation_instruction_2": "Weka odometer na saa za injini moja kwa moja kutoka dashibodi.",
  "step_pre_trip_documentation_instruction_3": "Andika hali ya jumla ya gari kwa picha za msingi na maelezo.",
  "step_pre_trip_documentation_instruction_4": "Thibitisha utambulisho wa gari dhidi ya karatasi za agizo kabla ya kuendelea.",

  "step_exterior_structure_title": "Ukaguzi wa Sehemu za Nje & Muundo",
  "step_exterior_structure_summary": "Fanya ukaguzi wa kuzunguka kwa kuzingatia mwanga wa mbele, pande, paa, na nyuma, ikiwa na picha zilizobainishwa.",
  "step_exterior_structure_instruction_1": "Fuata viashirio vya kuzunguka ili kufunika mbele, pande, mstari wa paa, na nyuma.",
  "step_exterior_structure_instruction_2": "Andika mnato, nyufa, kutu, au paneli zilizotelekezwa kwa ushahidi wa picha.",
  "step_exterior_structure_instruction_3": "Angalia reli za fremu, crossmembers, na pointi za uingizaji wa chasi kwa uharibifu wa muundo.",
  "step_exterior_structure_instruction_4": "Ambatanisha picha kama ushahidi kwa kasoro za nje au za muundo kabla ya kuendelea.",

  "step_tires_wheels_axles_title": "Tayari, Magurudumu, & Axles",
  "step_tires_wheels_axles_summary": "Pima na rekodi afya ya tairi, vifaa vya magurudumu, na hali ya axle kwa kila nafasi ya gurudumu.",
  "step_tires_wheels_axles_instruction_1": "Pima na rekodi shinikizo la tairi kwa kila nafasi ya gurudumu.",
  "step_tires_wheels_axles_instruction_2": "Pata kina cha tread na andika mifumo isiyo ya kawaida ya kuvaa.",
  "step_tires_wheels_axles_instruction_3": "Angalia mwando wa pembeni kwa kukata, kupasuka, au kuonekana kwa nyuzi na andika kasoro.",
  "step_tires_wheels_axles_instruction_4": "Angalia rim za magurudumu, viashiria vya torque za lug, axles, na bearings kwa msongamano au kuvuja.",

  "step_braking_system_title": "Mfumo wa Breki",
  "step_braking_system_summary": "Pima vipengele vya breki za huduma, dharura, na park na ushahidi wa hali ya kuvaa.",
  "step_braking_system_instruction_1": "Angalia pads za breki au shoes, drums, na rotors kwa unene na dalili za moto.",
  "step_braking_system_instruction_2": "Andika vipengele vya breki za majimaji au hewa, hose, na fittings kwa kuvuja au kuvaa.",
  "step_braking_system_instruction_3": "Angalia mistari ya usambazaji, mabomba, na valves kwa kuchafuka au upangaji usio sahihi.",
  "step_braking_system_instruction_4": "Jaribu matumizi ya breki ya park na rekodi ufanisi.",

  "step_suspension_steering_title": "Suspension & Steering",
  "step_suspension_steering_summary": "Tathmini utulivu na vipengele vya upangaji upangaji, kuhakikisha utendaji salama wa kushughulikia.",
  "step_suspension_steering_instruction_1": "Angalia springs, shock absorbers, na airbags kwa kuvuja, nyufa, au mwinuko wa usawa.",
  "step_suspension_steering_instruction_2": "Angalia linkage ya udhibiti, tie rods, na kingpins kwa mchezo au uharibifu.",
  "step_suspension_steering_instruction_3": "Andika mounts za suspension, bushings, torque arms, na u-bolts kwa kuvaa.",
  "step_suspension_steering_instruction_4": "Thibitisha viashirio vya ulinganifu na uwekaji wa gurudumu la mwelekeo.",

  "step_engine_powertrain_title": "Injini & Powertrain",
  "step_engine_powertrain_summary": "Pata afya ya bay ya injini ikijumuisha mafuta, belts, na uimara wa drivetrain.",
  "step_engine_powertrain_instruction_1": "Thibitisha kiwango cha mafuta, coolant, transmishe, DEF, na mafuta ya majimaji ni ndani ya kizingiti.",
  "step_engine_powertrain_instruction_2": "Angalia belts, hoses, clamps, na vichujio vya hewa kwa nyufa au ustawi mdogo.",
  "step_engine_powertrain_instruction_3": "Andika hali ya mfumo wa kutolea hewa, mounts, na dalili za kuvuja.",
  "step_engine_powertrain_instruction_4": "Angalia vipengele vya drivetrain, seals, na housings kwa kuvuja au kelele zisizo za kawaida.",

  "step_electrical_lighting_title": "Umeme & Taa",
  "step_electrical_lighting_summary": "Jaribu mfululizo wa taa na mifumo ya msingi ya umeme kwa mpangilio.",
  "step_electrical_lighting_instruction_1": "Fanya jaribio la taa kwa headlights, tail lights, na brake lights.",
  "step_electrical_lighting_instruction_2": "Geuza ishara za kugeuza, taa za alama, na flashers za hatari kuthibitisha ulinganifu.",
  "step_electrical_lighting_instruction_3": "Angalia taa za ndani, vyombo vya dashibodi, na viashirio vya tahadhari.",
  "step_electrical_lighting_instruction_4": "Angalia betri, harness ya waya, na connectors kwa kutu au terminals zilizolegea.",

  "step_cabin_interior_title": "Kabati & Ndani",
  "step_cabin_interior_summary": "Thibitisha vifaa vya usalama vya dereva, mfumo wa mwonekano, na utayari wa paneli ya udhibiti.",
  "step_cabin_interior_instruction_1": "Thibitisha mikanda ya kiti, reli za kiti, na headrests kwa uendeshaji salama.",
  "step_cabin_interior_instruction_2": "Angalia vioo, windshield, wipers, washers, na defrosters kwa mwonekano.",
  "step_cabin_interior_instruction_3": "Jaribu vyombo vya dashibodi, controls, HVAC, na mifumo ya habari.",
  "step_cabin_interior_instruction_4": "Thibitisha vifaa vya dharura, karatasi, na signage ya usalama zinaweza kufikiwa.",

  "step_coupling_connections_title": "Coupling & Connections",
  "step_coupling_connections_summary": "Thibitisha coupling ya trela, mistari, na uhusiano wa usalama inapohitajika.",
  "step_coupling_connections_instruction_1": "Angalia fifth wheel, kingpin, pintle, au gooseneck kwa kufungwa salama na kuvaa.",
  "step_coupling_connections_instruction_2": "Thibitisha minyororo ya usalama, waya za kuvunja, na pini za kufunga zipo na hazijaharibika.",
  "step_coupling_connections_instruction_3": "Angalia connectors za umeme, glad hands, na mistari ya hewa kwa kuvuja au msongamano.",
  "step_coupling_connections_instruction_4": "Pata picha za msaada kwa vifaa vya coupling vilivyokosa au vilivyovaliwa.",

  "step_safety_equipment_title": "Vifaa vya Usalama",
  "step_safety_equipment_summary": "Thibitisha vifaa vya dharura vipo, vimekusanywa, na ndani ya tarehe za huduma.",
  "step_safety_equipment_instruction_1": "Angalia kiwango cha chaji cha mzima wa moto, tag ya ukaguzi, na muhuri wa usalama.",
  "step_safety_equipment_instruction_2": "Thibitisha pembeni vya tahadhari, mwanga wa tahadhari, na vifaa vya kuonyesha vyako vinapatikana na vinafaa.",
  "step_safety_equipment_instruction_3": "Thibitisha seti ya kwanza ya msaada, seti ya kuvuja, na hesabu ya PPE ni kamili.",
  "step_safety_equipment_instruction_4": "Andika vifaa vya dharura vinavyokosa au vya ubora mdogo kwa picha.",

  "step_operational_tests_title": "Vipimo vya Uendeshaji",
  "step_operational_tests_summary": "Maliza ukaguzi na vipimo vya uendeshaji kabla ya kuachilia gari.",
  "step_operational_tests_instruction_1": "Fanya jaribio la utendakazi wa breki ukitambua hisia za pedal na umbali wa kusimama.",
  "step_operational_tests_instruction_2": "Geuza mwelekeo wa udhibiti kusikiliza kelele zisizo za kawaida au upinzani.",
  "step_operational_tests_instruction_3": "Washa injini, angalia ubora wa idle, taa za onyo, na vibration.",
  "step_operational_tests_instruction_4": "Fanya jaribio la kubadilisha gia kuthibitisha uendeshaji laini katika anuwai zote."
}
